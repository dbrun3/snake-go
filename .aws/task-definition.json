{
  "family": "snake-go",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "executionRoleArn": "ROLE_ARN",
  "containerDefinitions": [
    {
      "name": "snake-go-server",
      "image": "IMAGE_URI",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ],
  "cpu": "512",
  "memory": "1024"
}
